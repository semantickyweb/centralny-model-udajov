esco:Relationship  rdf:type  owl:Class;
                   rdfs:comment      "The class of directed relationships between two ESCO Pillar concepts (e.g. between an esco:Occupation and an esco:Qualification).\n\nThe relationship:\n- starts from the ESCO concept identified by http://data.europa.eu/esco/model#isRelationshipFor.\n- is detailed using a descriptive text (http://purl.org/dc/terms/description).  This description may contain reference to external sources (see http://data.europa.eu/esco/model#relatedLegallyRequiredQualification)\n- ends in the ESCO concept identified by http://data.europa.eu/esco/model#refersConcept.\n- is typed by the tagging property http://data.europa.eu/esco/model#hasRelationshipType.\nA relationship may be industry sector or location specific. This can be acieved by tagging the relationship using esco:hasNACECode or esco:location respectively."@en;
                   rdfs:isDefinedBy  <http://data.europa.eu/esco/model>;
                   rdfs:label        "ESCO concepts Relationship"@en;
                   rdfs:seeAlso      esco:relatedLegallyRequiredQualification , esco:hasRelationshipType , esco:isRelationshipFor , esco:refersConcept;
                   rdfs:subClassOf   [ rdf:type                  owl:Restriction;
                                       owl:onClass               esco:Concept;
                                       owl:onProperty            esco:isRelationshipFor;
                                       owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
                             ];
                   rdfs:subClassOf   [ rdf:type                  owl:Restriction;
                                       owl:onClass               esco:Concept;
                                       owl:onProperty            esco:refersConcept;
                                       owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
                             ];
                   terms:issued      "2013-12-03"^^xsd:date;
                   terms:modified    "2017-02-10"^^xsd:date;
                   owl:deprecated    true .

