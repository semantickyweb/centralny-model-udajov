esco:statusDataType  rdf:type  rdfs:Datatype;
                     rdfs:comment         "list of status values used for managing ESCO release status.\nOn creation of a concept or label, the default state is \"to be reviewed\"\nOn deprecation of a concept the concept state becomed \"obsolete\"\nWhen a wrongly deprecated concept is revived, the state becomes \"to be reviewed\"\nWhen the thesaurus is published, the statuss becomes \"released\"\n\nAll state changes are system managed."@en;
                     rdfs:isDefinedBy     <http://data.europa.eu/esco/model>;
                     rdfs:label           "thesaurus concept and label publication status type"@en;
                     terms:modified       "2013-12-05";
                     owl:equivalentClass  [ rdf:type     rdfs:Datatype;
                                            owl:unionOf  ( [ rdf:type              rdfs:Datatype;
                                                             owl:onDatatype        xsd:string;
                                                             owl:withRestrictions  ( [ xsd:pattern  "obsolete" ]
                                                                                   )
                                                           ]
